class sub {
public static int[]  add(int [] a){
  int sum = 0; 
 int i = 0;

for(i = 0; i < a.length; i++) {
    sum = sum + a[i];
}

return new int[] {sum}; 

}

public static void main (String [] args){
    int [] a = {4,-3,2,8,-1};
    int [] result = add(a);
    System.out.println(Arrays.toString(result));
}
}
